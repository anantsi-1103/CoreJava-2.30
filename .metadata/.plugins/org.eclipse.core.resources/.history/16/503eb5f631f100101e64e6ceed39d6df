package OOPs;

public class oops4 {
	public static void main(String[] args) {
		// object creation
		Student_form2 s1 = new Student_form2();

		// getter
		System.out.println(s1.getName());
		System.out.println(s1.getAge());
		System.out.println(s1.getCity());

	}
}

class Student_form2 {

	public Student_form2() {
		System.out.println("Constructor is called ");
	}

	public Student_form2(String name) {
		System.out.println("Constructor with name is called " + name);
	}

	public Student_form2(String name, String city, int age) {
		this.age = age;
		this.city = city;
		this.name = name;
	}

	// data members ->
	String name; //
	int age;
	String city;

	public String getName() {
		return name;
	}

	public int getAge() {
		return age;
	}

	public String getCity() {
		return city;
	}

}
