package OOPs;

public class oops1 {
	public static void main(String[] args) {
		Student_form s1 = new Student_form();
		
		s1.addName("RAM");
		s1.addCity("Delhi");
		s1.addAge(20);
		
		
		
	}
}

class Student_form {

	// data members ->
	String name; // xyz
	int age;
	String city;

	// function ->
	void addName(String newName) {
		this.name = newName;
	}

	void addAge(int newAge) {
		this.age = newAge;
	}

	void addCity(String newCity) {
		this.city = newCity;
	}
}
