package OOPs;

public class oops4 {
	public static void main(String[] args) {
		// object creation
		Student_form2 s1 = new Student_form2();

		// setter
		s1.setName("RAM");
		s1.setCity("Delhi");
		s1.setAge(20);

		// getter
		System.out.println(s1.getName());
		System.out.println(s1.getAge());
		System.out.println(s1.getCity());

		

	}
}

class Student_form2 {

	// data members ->
	String name; //
	int age;
	String city;

	public String getName() {
		return name;
	}

	public void setName(String name) {
		this.name = name;
	}

	public int getAge() {
		return age;
	}

	public void setAge(int age) {
		this.age = age;
	}

	public String getCity() {
		return city;
	}

	public void setCity(String city) {
		this.city = city;
	}

}
