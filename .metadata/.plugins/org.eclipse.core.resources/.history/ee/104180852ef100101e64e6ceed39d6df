package OOPs;

public class oops2 {
	public static void main(String[] args) {
		// object creation
		Student_form1 s1 = new Student_form1();

		// setter
		s1.addName("RAM");
		s1.addCity("Delhi");
		s1.addAge(20);

		// getter
		System.out.println(s1.getName());
		System.out.println(s1.getAge());
		System.out.println(s1.getCity());

		// object new object
		Student_form1 s2 = new Student_form1();

		// setter
		s2.addName("Aman");
		s2.addCity("Jaipur");
		s2.addAge(22);

		// getter
		System.out.println(s2.getName());
		System.out.println(s2.getAge());
		System.out.println(s2.getCity());

	}
}

class Student_form1{

	// data members ->
	String name; //
	int age;
	String city;
	public String getName() {
		return name;
	}
	public void setName(String name) {
		this.name = name;
	}
	public int getAge() {
		return age;
	}
	public void setAge(int age) {
		this.age = age;
	}
	public String getCity() {
		return city;
	}
	public void setCity(String city) {
		this.city = city;
	}
	
	
	
}
	
